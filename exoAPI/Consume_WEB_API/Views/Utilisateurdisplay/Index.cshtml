@model IEnumerable<Consume_WEB_API.Models.Utilisateur>

@{
    ViewBag.Title = "Utilisateur";
}

<center>
    <h2>Liste d'utilisateur'</h2>


    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Civilitie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Firstame)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SecuNb)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Civilitie)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Firstame)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SecuNb)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
</center>
